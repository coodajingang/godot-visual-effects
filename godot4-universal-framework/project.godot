; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format: https://docs.godotengine.org/en/stable/tutorials/io/resource_format.html

config_version=5

[application]
config/name="Godot4 Universal Game Framework"
run/main_scene="res://scenes/Main.tscn"
config/icon="res://icon.svg"

[autoload]
DebugConsole="*res://addons/universal_framework/devtools/debug_console.gd"
GameApp="*res://framework/core/game_app.gd"

[display]
window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[editor_plugins]
enabled=PackedStringArray("res://addons/universal_framework/plugin.cfg")

[rendering]
environment/default_environment="res://default_env.tres"
